{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "warn",
    "import/no-unresolved": 0,
    "no-console": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-underscore-dangle": 0,
    "curly": ["error", "multi-line"]
  }
}
